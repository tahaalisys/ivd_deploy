{
  "version": 2,
  "builds": [{ "src": "src/index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
  "files": [
    "src/**",
    {
      "source": "node_modules/geoip-lite/data/geoip-country.dat",
      "destination": "/var/task/node_modules/geoip-lite/data/geoip-country.dat"
    }
  ]
}
